{
  "name": "react-twitter-clone-backend",
  "version": "1.0.0",
  "description": "twitter clone ",
  "main": "app.js",
  "scripts": {
    "debug": "SET DEBUG=twitterclone,app:* & nodemon app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client-install": "npm install --prefix client",
    "start": "node app.js",
    "client-build": "npm run build --prefix client",
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lollykrown/react-twitter-clone.git"
  },
  "keywords": [
    "twitter",
    "react",
    "express",
    "node",
    "mongoDb"
  ],
  "author": "Kayode Agboola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lollykrown/react-twitter-clone/issues"
  },
  "homepage": "https://github.com/lollykrown/react-twitter-clone#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "passport-local": "^1.0.0",
    "react-toastify": "^6.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
